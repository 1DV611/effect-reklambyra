<div class="card dashboard-content-place">

    <!--- Välj en kund --->
    <div class="report-settings">
        <form id="customer-statistics" method="post" action="/user/preview">

                <div class="customer-container">
                <div class="customer-label">
                    <span>Kund </span>
                </div>
                <div class="customer-input">
                    {{#if user.admin}}
                    <input class="form-control" id="customer" name="customer" list="customer-datalist" type="text" value="" />
                        <datalist id="customer-datalist"></datalist>
                    {{else}}
                        <input class="form-control" id="customer" name="customer" list="customer-datalist" type="text" value="{{user.displayName}}" />
                    {{/if}}
                </div>
            </div>
            <div class="social-container">
            <div id="datepicker">
                <div id="yearpicker">
                    <div class="social-name">
                        <span>År </span>
                        <select id="years" name="years">
                            {{#if user.admin}}
                            {{else}}
                                {{#each dates}}
                                    <option value="{{@key}}" selected>{{@key}}</option>
                                {{/each}}
                            {{/if}}
                        </select>
                    </div>
                </div>
                <div id="monthpicker">
                    <div class="social-name">
                        <span>Månad </span>
                        <select id="months" name="months">
                            {{#if user.admin}}
                            {{else}}
                                {{#each dates}}
                                    {{optionTemplate @key this}}
                                {{/each}}
                            {{/if}}
                        </select>
                    </div>
                </div>
            </div>
            </div>
            <!--- Välj medier, features, månad och år för preview: --->
            <div class="social-container">
                <div class="social-name"><span>AddThis</span></div>
                <div class="social-switch">
                    <input id="addthis" name="addthis" class="cmn-toggle cmn-toggle-round" type="checkbox" checked>
                    <label for="addthis"></label>
                </div>
                <div class="social-features">
                    <input id="addthis-click" name="addthisClick" type="checkbox" checked>
                    <label for="addthisClick">Klick </label>
                </div>
            </div>
            <div class="social-container">
                <div class="social-name"><span>Adwords</span></div>
                <div class="social-switch">
                    <input id="adwords" name="adwords" class="cmn-toggle cmn-toggle-round" type="checkbox" checked>
                    <label for="adwords"></label>
                </div>
                <div class="social-features">
                    <input id="adwords-click" name="adwordsClick" type="checkbox" checked>
                    <label for="adwordsClick">Klick </label>
                    <input id="adwords-cpc" name="adwordsCpc" type="checkbox" checked>
                    <label for="adwordsCpc">CPC </label>
                    <input id="adwords-views" name="adwordsViews" type="checkbox" checked>
                    <label for="adwordsViews">Visningar </label>
                </div>
            </div>
            <div class="social-container">
                <div class="social-name"><span>Analytics</span></div>
                <div class="social-switch">
                    <input id="analytics" name="analytics" class="cmn-toggle cmn-toggle-round" type="checkbox" checked>
                    <label for="analytics"></label>
                </div>
                <div class="social-features">
                    <input id="analytics-views" name="analyticsViews" type="checkbox" checked>
                    <label for="analyticsViews">Visningar </label>
                    <input id="analytics-unique-views" name="analyticsUniqueViews" type="checkbox" checked>
                    <label for="analyticsUniqueViews">Unika visningar </label>
                    <input id="analytics-4-strongest-redirects" name="analyticsStrongestRedirects" type="checkbox" checked>
                    <label for="analyticsStrongestRedirects">4 starkaste ingångskanalerna </label>
                    <input id="analytics-4-most-visited-pages" name="analyticsMostVisitedPages" type="checkbox" checked>
                    <label for="analyticsMostVisitedPages">4 mest besökta sidorna </label>
                    <input id="analytics-average-time" name="analyticsAverageTime" type="checkbox" checked>
                    <label for="analyticsAverageTime">Genomsnittlig tid på sidan </label>
                    <input id="analytics-average-visited-per-pages" name="analyticsAverageVisitedPerPages" type="checkbox" checked>
                    <label for="analyticsAverageVisitedPerPages">Genomsnittligt antal besökta sidor </label>
                </div>
            </div>
            <div class="social-container">
                <div class="social-name"><span>Facebook</span></div>
                <div class="social-switch">
                    <input id="facebook" name="facebook" class="cmn-toggle cmn-toggle-round" type="checkbox" checked>
                    <label for="facebook"></label>
                </div>
                <div class="social-features">
                    <input id="facebook-likes" name="facebookLikes" type="checkbox" checked>
                    <label for="facebookLikes">Gilla </label>
                </div>
            </div>
            <div class="social-container">
                <div class="social-name"><span>Instagram</span></div>
                <div class="social-switch">
                    <input id="instagram" name="instagram" class="cmn-toggle cmn-toggle-round" type="checkbox" checked>
                    <label for="instagram"></label>
                </div>
                <div class="social-features">
                    <input id="instagram-likes" name="instagramLikes" type="checkbox" checked>
                    <label for="instagramLikes">Gilla </label>
                    <input id="instagram-comments" name="instagramComments" type="checkbox" checked>
                    <label for="instagramComments">Kommentarer </label>
                    <input id="instagram-tags" name="instagramTags" type="checkbox" checked>
                    <label for="instagramTags">Taggningar </label>
                    <input id="instagram-taggedUsers" name="instagramTaggedUsers" type="checkbox" checked>
                    <label for="instagramTaggedUsers">Taggade användare </label>
                </div>
            </div>
            <div class="social-container">
                <div class="social-name"><span>LinkedIn</span></div>
                <div class="social-switch">
                    <input id="linkedin" name="linkedin" class="cmn-toggle cmn-toggle-round" type="checkbox" checked>
                    <label for="linkedin"></label>
                </div>
                <div class="social-features">
                    <input id="linkedin-followers" name="linkedinFollowers" type="checkbox" checked>
                    <label for="linkedinFollowers">Följare </label>
                    <input id="linkedin-interactions" name="linkedinInteractions" type="checkbox" checked>
                    <label for="linkedinInteractions">Interaktioner </label>
                </div>
            </div>
            <div class="social-container">
                <div class="social-name"><span>Twitter</span></div>
                <div class="social-switch">
                    <input id="twitter" name="twitter" class="cmn-toggle cmn-toggle-round" type="checkbox" checked>
                    <label for="twitter"></label>
                </div>
                <div class="social-features">
                    <input id="twitter-followers" name="twitterFollowers" type="checkbox" checked>
                    <label for="twitterFollowers">Följare </label>
                </div>
            </div>
            <div class="social-container">
                <div class="social-name"><span>Tynt/33across</span></div>
                <div class="social-switch">
                    <input id="across" name="across" class="cmn-toggle cmn-toggle-round" type="checkbox" checked>
                    <label for="across"></label>
                </div>
                <div class="social-features">
                    <input id="across-shares" name="acrossShares" type="checkbox" checked>
                    <label for="acrossShares">Delningar </label>

                    <input id="across-visitors" name="acrossVisitors" type="checkbox" checked>
                    <label for="acrossVisitors">Besökare </label>
                </div>
            </div>
            <div class="social-container">
                <div class="social-name"><span>Youtube</span></div>
                <div class="social-switch">
                    <input id="youtube" name="youtube" class="cmn-toggle cmn-toggle-round" type="checkbox" checked>
                    <label for="youtube"></label>
                </div>
                <div class="social-features">
                    <input id="youtube-views" name="youtubeViews" type="checkbox" checked>
                    <label for="youtubeViews">Visningar </label>
                </div>
            </div>
            <br>
            <div class="left-button-div">
                <a id="previewButton" href="" onclick="changeHrefPreview()" class="btn btn-default">Förhandsvisa månadsrapport</a>
            </div>

            <br>

            <!--- Möjlig utvigning av dashboard (ej fullt ut testad):
            {{#if this.user.admin}}
                <div class="left-button-div">
                    <a id="showAllReportsButton" href="" onclick="changeHrefReports()" class="btn btn-default">Visa alla månadsrapporterna</a>
                </div>
            {{/if}}
            --->
            {{#if user.admin}}
            <script src="/js/lib/dashboardAjax/adminGetUsersForAutoComplete.js"></script>
            <script src="/js/lib/dashboardAjax/adminPopulateForSelectedUser.js"></script>
            {{else}}
            <script src="/js/lib/populateYearAndMonthSelectsForNonAdminUser.js"></script>
            {{/if}}

            <script src="/js/changeHrefDashboard.js"></script>
        </form>
    </div>
</div>
